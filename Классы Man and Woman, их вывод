public class Solution
{
    public static void main(String[] args)
    {
        //создай по два объекта каждого класса тут
        Man sasha = new Man("Саша", 16, "Весь мир - его нора");
        Man pasha = new Man("Паша", 19, "Его дом - его крепость, и никто не знает, где она");

        Woman Sasha = new Woman("Александра", 17, "Дом, её милый дом - тёплая постель");
        Woman Masha = new Woman("Маша", 17, "Пица, БК и секс");

        System.out.println(sasha.name + " " + sasha.age + " " + sasha.address);
        System.out.println(pasha.name + " " + pasha.age + " " + pasha.address);
        System.out.println(Sasha.name + " " + Sasha.age + " " + Sasha.address);
        System.out.println(Masha.name + " " + Masha.age + " " + Masha.address);
    }


    public static class Man
    {
        String name;
        int age;
        String address;

        Man(String name, int age, String address)
        {
            this.name = name;
            this.age = age;
            this.address = address;
        }

    }
    public static class Woman
    {
        String name;
        int age;
        String address;

        Woman(String name, int age, String address)
        {
            this.name = name;
            this.age = age;
            this.address = address;
        }
    }
}
